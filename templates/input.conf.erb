[[input.<%= @name %> ]]
<% if defined?(@options)
     @options.keys.sort.each do |k| -%>
<%= "#{k} = #{@options[k]}" %>
<%   end
   end -%>
<% if defined?(@sections)
     @sections.keys.each do |k| -%>

[[input.<%= "#{k}" %>]]
<%     @sections[k].keys.sort.each do |v| -%>
<%= "#{v} = #{@sections[k][v]}" %>
<%     end
     end
   end -%>
